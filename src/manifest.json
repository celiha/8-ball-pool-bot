{
    "name": "8 Ball Pool Cheats",
    "description": "Cheats for Miniclip's 8 Ball Pool",
    "version": "0.1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://prod-web-pool.miniclip.com/*"
            ],
           "js": [
                "js/inject.js"
            ],
            "all_frames": true,
            "run_at" : "document_end"
        }
    ],
    "web_accessible_resources": [
        "js/webgl-context-wrapper.js"
    ]
}
